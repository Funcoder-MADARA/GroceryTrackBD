{
  "name": "grocery-track-bd-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for GroceryTrackBD - Backend and Frontend",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run start:frontend\"",
    "dev": "nodemon server.js",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd frontend && npm install"
  },
  "keywords": [
    "grocery",
    "bangladesh",
    "shop-management",
    "delivery",
    "monorepo"
  ],
  "author": "GroceryTrackBD Team",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "twilio": "^4.15.0"
  },
  "devDependencies": {
    "@types/react-dom": "^19.1.6",
    "concurrently": "^8.2.2",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1"
  }
}
